    "esdoc": "1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "6.5.1",
    "express": "4.17.1",
    "handlebars": "4.5.3",
    "handlebars-loader": "1.7.1",
    "mini-css-extract-plugin": "0.8.0",
    "path": "0.12.7",
    "rimraf": "3.0.0",
    "style-loader": "1.0.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7",
    "webpack-merge": "4.2.1"
