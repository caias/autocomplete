    "es2015",
    "es6",
    "autocomplete",
    "input",
    "jquery"
  ],
  "author": "caias",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "esdoc-node": "1.0.5",
    "generate-changelog": "1.8.0",
    "html-webpack-plugin": "3.2.0",
    "lerna": "3.10.5",
    "lodash": "4.17.19",
    "mocha": "6.2.0",
    "nodemon": "1.19.1"
  },
  "dependencies": {
    "@otom/elements": "file:packages/elements",
