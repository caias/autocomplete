{
  "name": "fe-otom",
  "version": "1.0.0",
  "description": "autocomplete module using javascript",
  "main": "dist/otom.js",
  "scripts": {
    "test": "mocha ./test/otom.spec.js",
    "dev": "webpack --config ./tools/development.js",
    "prod": "webpack --config ./tools/production.js",
    "example": "nodemon ./example/server.js",
